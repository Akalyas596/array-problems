public class maxminarray {
    public static void main(String[] args){
        int arr[] = {33,3,4,7,23};
        int max=0,min=arr[0];
        for(int i=0;i<arr.length;i++){
            if(arr[i]>max)
            {
                max=arr[i];
                System.out.println(max);
            }
            if(arr[i]<min)
            {
                min=arr[i];
                System.out.println(min);
            }
        }
    }
}
