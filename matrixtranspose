public class matrixtranspose {
    public static void main(String[] args){
        int ar[][]={{1,2,3},{4,5,6},{7,8,9}};
        int tr[][]=new int[3][3];
        for(int i=0;i<3;i++){
            for(int j=0;j<3;j++){
                tr[j][i]=ar[i][j];
            }}
            for(int i=0;i<3;i++) {
                for (int j = 0; j < 3;j++) {
              System.out.print(tr[i][j]+" ");
                }
                System.out.println();
            }

        }
        }
